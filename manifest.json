{
   "content_scripts": [ {
      "css": [ "readable.css" ],
      "js": [ "jquery.js", "jquery.hotkeys.js", "enhancements.js" ],
      "matches": [ "http://en.wikipedia.org/wiki/*" ]
   } ],
   "description": "Enhacements to wikipedia to increase its readability.",
   "icons": {
      "128": "icon_128.png",
      "48": "icon_48.png"
   },
   "manifest_version": 2,
   "name": "Wikipedia Readability",
   "permissions": [ "tabs", "http://en.wikipedia.org/wiki/*" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.5",
   "web_accessible_resources": [ "images/desktop.png", "images/mobile.gif" ]
}
